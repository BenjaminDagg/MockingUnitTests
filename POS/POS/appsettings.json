{
  "ConnectionStrings": {
    "ApplicationConnectionString": "#{ApplicationConnectionString}#"
  },
  "Logging": {
    "LogLevel": {
      "Default": "#{DefaultLogLevel}#",
      "System": "Error",
      "Microsoft": "#{MicrosoftLogLevel}#"
    }
  },
  "DefaultAccount": {
    "Role": "#{DefaultAccountRole}#",
    "UserName": "#{DefaultAccountUserName}#",
    "Email": "#{DefaultAccountEmail}#",
    "PhoneNumber": "#{DefaultAccountPhoneNumber}#",
    "FirstName": "#{DefaultAccountFirstName}#",
    "LastName": "#{DefaultAccountLastName}#"
  },
  "UserPrinterSettings": {
    "ReportPrinterName": "",
    "ReceiptPrinterName": ""
  },
  "UserVoucherSettings": {
    "VoucherCharacterLength": "18"
  },
  "DeviceManagerSettings":{
    "PollingInterval":"#{PollingInterval}#",
    "ConnectionTimeOut": "#{ConnectionTimeOut}#",
    "ServiceEndPoint": "#{ServiceEndPoint}#",
    "ServicePort": "#{ServicePort}#"
  },
  "StartUpMode": {
    "DisplayMode": "#{DisplayMode}#" //Can be NoResize or CanResize
  },
  "TestDbRetryConfiguration": {
    "NumberTimesToRetry": "#{NumberTimesToRetry}#",
    "SecondsToWait": "#{SecondsToWait}#"
  }
}