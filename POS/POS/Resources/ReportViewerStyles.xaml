    <Style TargetType="{x:Type xaml:ReportViewer}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type xaml:ReportViewer}">
                    <Border BorderThickness="1" BorderBrush="Black">
                        <Grid x:Name="PART_MainGrid" Background="Black" Height="Auto" Margin="0">
                            <Grid.RowDefinitions>
                                <RowDefinition x:Name="PART_toolBarGridRow" Height="40"/>
                                <RowDefinition x:Name="PART_dsCredentialRow" Height="Auto"/>
                                <RowDefinition x:Name="PART_parameterGridRow" Height="Auto"/>
                                <RowDefinition x:Name="PART_viewerContentRow" Height="*"/>
                                <RowDefinition x:Name="PART_gridExceptionRow" Height="0"/>
                                <RowDefinition x:Name="PART_loadingIndicatorRow" Height="0"/>
                            </Grid.RowDefinitions>
                            <Border BorderThickness="0,0,0,1" BorderBrush="Black">
                                <StackPanel x:Name="PART_toolBar" Background="Black" Height="40" Margin="0" Orientation="Horizontal">
                                    <Button x:Name="PART_buttonPrint" Height="30" IsEnabled="False" Margin="5,0,5,0" Style="{StaticResource POSOffice2013ButtonStyle}" ToolTip="{x:Static prop:Resources.PrintButtonToolTip}" Width="30">
                                        <Image Height="16" Stretch="None" Width="16">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled"  Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource POSOffice2013PrintButtonDisabled}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled"   Value="True">
                                                            <Setter Property="Image.Source"  Value="{StaticResource POSOffice2013PrintButton}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
