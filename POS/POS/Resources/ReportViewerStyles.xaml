        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type xaml:ReportViewer}">
                    <Border BorderThickness="1" BorderBrush="Black">
                        <Grid x:Name="PART_MainGrid" Background="Black" Height="Auto" Margin="0">
                            <Grid.RowDefinitions>
                                <RowDefinition x:Name="PART_toolBarGridRow" Height="40"/>
                                <RowDefinition x:Name="PART_dsCredentialRow" Height="Auto"/>
                                <RowDefinition x:Name="PART_parameterGridRow" Height="Auto"/>
                                <RowDefinition x:Name="PART_viewerContentRow" Height="*"/>
                                <RowDefinition x:Name="PART_gridExceptionRow" Height="0"/>
                                <RowDefinition x:Name="PART_loadingIndicatorRow" Height="0"/>
                            </Grid.RowDefinitions>
                            <Border BorderThickness="0,0,0,1" BorderBrush="Black">
                                <StackPanel x:Name="PART_toolBar" Background="Black" Height="40" Margin="0" Orientation="Horizontal">

                                    <Button x:Name="PART_buttonPrint" Height="30" IsEnabled="False" Margin="5,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.PrintButtonToolTip}" Width="30">
                                        <Image Height="16" Stretch="None" Width="16">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled"  Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013PrintButtonDisabled}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled"   Value="True">
                                                            <Setter Property="Image.Source"  Value="{StaticResource Office2013PrintButton}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                    <controls:CustomComboBoxControlEx x:Name="PART_exportControl" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ExportComboBox}" ItemContainerStyle="{StaticResource Office2013ItemTemplate}" SnapsToDevicePixels="True" ToolTip="Export" Width="50" MinWidth="30"/>
                                    <Rectangle Fill="{StaticResource DgAccent}" HorizontalAlignment="Center" Height="30" Margin="0,0,5,0" VerticalAlignment="Center" Width="1">

                                    </Rectangle>
                                    <Button x:Name="PART_buttonFirst" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.FirstPageButtonToolTip}" Width="30">
                                        <Image Height="11" Stretch="Fill" Width="11">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger  Property="Button.IsEnabled"  Value="False">
                                                            <Setter Property="Image.Source"  Value="{StaticResource Office2013NavFirstDis}"/>
                                                        </Trigger>
                                                        <Trigger Value="True"  Property="Button.IsEnabled" >
                                                            <Setter Property="Image.Source"  Value="{StaticResource Office2013NavFirst}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                    <Button x:Name="PART_buttonPrevious" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.PreviousPageButtonToolTip}" Width="30">
                                        <Image Height="11" Stretch="Fill" Width="11">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Value="False"  Property="Button.IsEnabled" >
                                                            <Setter Property="Image.Source"  Value="{StaticResource Office2013NavPreviousDis}"/>
                                                        </Trigger>
                                                        <Trigger  Property="Button.IsEnabled"  Value="True">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013NavPrevious}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                    <TextBox x:Name="PART_textBoxCurrentPage" BorderBrush="#ababab" FontSize="14" Height="25" IsEnabled="False" IsReadOnly="False" MinWidth="30" Margin="0,0,5,0" MaxWidth="60" Padding="0" TextAlignment="Center" VerticalContentAlignment="Center"/>
                                    <TextBlock x:Name="PART_labelOf" Foreground="White" FontSize="14" Margin="0,0,5,0" Text="{x:Static prop:Resources.PageOf}" VerticalAlignment="Center"/>
                                    <TextBlock x:Name="PART_textBoxTotalPages" Foreground="White" FontSize="14" HorizontalAlignment="Center" IsEnabled="false" Margin="0,0,5,0" TextAlignment="Center" VerticalAlignment="Center" Width="Auto"/>
                                    <Button x:Name="PART_buttonNext" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.NextPageButtonToolTip}" Width="30">
                                        <Image Height="11" Stretch="Fill" Width="11">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger  Property="Button.IsEnabled" Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013NavNextDis}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled" Value="True">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013NavNext}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                    <Button x:Name="PART_buttonLast" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.LastPageButtonToolTip}" Width="30">
                                        <Image Height="11" Stretch="Fill" Width="11">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled" Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013NavLastDis}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled" Value="True">
                                                            <Setter Property="Image.Source"  Value="{StaticResource Office2013NavLast}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                    <Button x:Name="PART_btnback" Height="30" IsEnabled="True" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.BackButtonToolTip}" Visibility="Collapsed" Width="30">
                                        <Image Height="16" Stretch="None" Source="{StaticResource Office2013ReportBack}" Width="16"/>
                                    </Button>
                                    <Rectangle Fill="{StaticResource DgAccent}" HorizontalAlignment="Center" Height="30" Margin="0,0,5,0" VerticalAlignment="Center" Width="1">

                                    </Rectangle>
                                    <Border x:Name="PART_comboBoxExternalBorder" BorderThickness="0" Height="30" Margin="5,0,5,0" ToolTip="{x:Static prop:Resources.ZoomPercentToolTip}" Width="80">
                                        <controls:CustomComboBoxControlEx x:Name="PART_comboBoxPageZoom"  FontSize="14" Height="30" IsEnabled="False" Style="{StaticResource DgSmallComboBoxStyle}" MinWidth="50" SnapsToDevicePixels="True" SelectedIndex="2" Width="80">
                                            <ComboBoxItem Content="25%"/>
                                            <ComboBoxItem Content="50%"/>
                                            <ComboBoxItem Content="100%"/>
                                            <ComboBoxItem Content="200%"/>
                                            <ComboBoxItem Content="300%"/>
                                            <ComboBoxItem Content="400%"/>
                                            <ComboBoxItem Content="500%"/>
                                        </controls:CustomComboBoxControlEx>
                                    </Border>
                                    <ToggleButton x:Name="PART_buttonPrintLayout" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013PrintLayoutButtonStyle}" ToolTip="{x:Static prop:Resources.PrintLayoutButtonToolTip}" Width="30">
                                        <Image Height="16" Stretch="None" Width="16">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled" Value="False">
                                                            <Setter Property="Image.Source"  Value="{StaticResource Office2013PrintLayoutXDisabled}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled" Value="True">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013PrintLayoutX}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </ToggleButton>
                                    <Button x:Name="PART_buttonPageSetup" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.PageUnitInchesValue}" Visibility="Visible" Width="30">
                                        <Image Height="16" Stretch="None" Width="16">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled" Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013PageSetupDisabled}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled" Value="True">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013PageSetup}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                    <Rectangle Fill="{StaticResource DgAccent}" HorizontalAlignment="Center" Height="30" Margin="0,0,5,0" VerticalAlignment="Center" Width="1">

                                    </Rectangle>
                                    <Button x:Name="PART_buttonRefresh" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.RefreshButtonToolTip}" Width="30">
                                        <Image HorizontalAlignment="Center" Height="16" Stretch="None" VerticalAlignment="Center" Width="16">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled" Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013RefreshButtonDisabled}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled" Value="True">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013RefreshButton}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                    <ToggleButton x:Name="PART_buttonShowOrHideDocumentMap" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ParameterButtonStyle}" ToolTip="{x:Static prop:Resources.DocumentMapButtonToolTip}" Visibility="Collapsed" Width="30">
                                        <Image HorizontalAlignment="Center" Height="16" Stretch="None" Source="{StaticResource Office2013DocumentMap}" VerticalAlignment="Center" Width="16"/>
                                    </ToggleButton>
                                    <ToggleButton x:Name="PART_buttonParameters" Background="Transparent" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ParameterButtonStyle}" ToolTip="{x:Static prop:Resources.ParameterAreaButtonToolTip}" Visibility="Collapsed" Width="30">
                                        <Image Height="16" Stretch="None" Width="16">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled" Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013ParameterButtonDis}"/>
                                                        </Trigger>
                                                        <Trigger Property="Button.IsEnabled" Value="True">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013ParameterButton}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </ToggleButton>
                                    <Rectangle Fill="{StaticResource DgAccent}" HorizontalAlignment="Center" Height="30" Margin="0,0,5,0" VerticalAlignment="Center" Visibility="Hidden" Width="1">

                                    </Rectangle>
                                    <TextBox x:Name="PART_textBoxFind" BorderBrush="Gray" Height="22" IsEnabled="False" Margin="0,0,5,0" ToolTip="{x:Static prop:Resources.FindButtonToolTip}" Visibility="Collapsed" Width="60"/>
                                    <Button x:Name="PART_buttonFind" Height="30" IsEnabled="False" Margin="0,0,5,0" Style="{StaticResource Office2013ButtonStyle}" ToolTip="{x:Static prop:Resources.FindButtonText}" Visibility="Collapsed" Width="30">
                                        <Image Height="16" Stretch="None" Width="16">
                                            <Image.Style>
                                                <Style>
                                                    <Style.Triggers>
                                                        <Trigger Property="Button.IsEnabled" Value="False">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013FindDisabled}"/>
                                                        </Trigger>
                                                        <Trigger  Property="Button.IsEnabled"  Value="True">
                                                            <Setter Property="Image.Source" Value="{StaticResource Office2013Find}"/>
                                                        </Trigger>
                                                    </Style.Triggers>
                                                </Style>
                                            </Image.Style>
                                        </Image>
                                    </Button>
                                </StackPanel>
                            </Border>
                            <Grid FlowDirection="LeftToRight" Grid.Row="1">
                                <Grid.RowDefinitions>
                                    <RowDefinition x:Name="PART_rowDefinitionFordsCredentialBlock" Height="Auto"/>
                                </Grid.RowDefinitions>
                                <ScrollViewer x:Name="PART_scorllDSCredentialBlock" HorizontalScrollBarVisibility="Disabled" HorizontalAlignment="Stretch" Visibility="Collapsed" VerticalScrollBarVisibility="Auto">
                                    <StackPanel x:Name="PART_stackPaneldsCredential" Background="LightBlue" Height="Auto" Width="Auto">
                                        <Grid>
                                            <Grid.ColumnDefinitions>
                                                <ColumnDefinition Width="300*"/>
                                                <ColumnDefinition Width="100*"/>
                                            </Grid.ColumnDefinitions>
                                            <Grid x:Name="PART_grid_DsCredentialblock"  Grid.Column="0" Width="Auto">
                                                <StackPanel x:Name="PART_sPanel_Head" Width="Auto"/>
                                            </Grid>
                                            <StackPanel>
                                                <Button x:Name="PART_btnViewReport1" Content="{x:Static prop:Resources.ViewReportButtonText}" Foreground="#444444" HorizontalAlignment="Right" Margin="0,5,20,5" Width="Auto"/>
                                            </StackPanel>
                                        </Grid>
                                    </StackPanel>
                                </ScrollViewer>
                            </Grid>


                            <Grid x:Name="PART_ParameterGrid" FlowDirection="LeftToRight"  MaxHeight="200" Grid.Row="2" Visibility="Collapsed">
                                <Grid.RowDefinitions>
                                    <RowDefinition x:Name="PART_rowDefinitionForParameterBlock" Height="Auto"/>
                                </Grid.RowDefinitions>
                                <Border Background="Black" BorderThickness="0,0,0,1" BorderBrush="Black">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*"/>
                                            <ColumnDefinition Width="Auto"/>
                                        </Grid.ColumnDefinitions>
                                        <ScrollViewer HorizontalScrollBarVisibility="Auto" HorizontalAlignment="Stretch" MaxHeight="200" VerticalScrollBarVisibility="Auto">
                                            <Grid x:Name="PART_grid_ReportParameterBlock" Background="Transparent" Grid.Column="0" Margin="5">
                                                <Grid.ColumnDefinitions>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                    <ColumnDefinition Width="Auto"/>
                                                </Grid.ColumnDefinitions>
                                            </Grid>
                                        </ScrollViewer>
                                        <StackPanel x:Name="Office2013ViewReportPanel">
                                            <Grid HorizontalAlignment="Right" Height="{Binding ActualHeight, ElementName=Office2013ViewReportPanel}" Width="120">
                                                <Border BorderThickness="1,0,0,0" BorderBrush="{StaticResource DgAccent}" Height="{Binding ActualHeight, ElementName=Office2013ViewReportPanel}" Margin="0,8,0,8"/>
                                                <controls:CustomButtonControlEx Style="{StaticResource DgCustomButtonControlExStyle}" Background="{StaticResource DgDefaultButtonBackground}" x:Name="PART_btnViewReport" Content="{x:Static prop:Resources.ViewReportButtonText}" HorizontalAlignment="Center" Height="27" VerticalContentAlignment="Center" Width="90"/>
                                            </Grid>
                                        </StackPanel>
                                    </Grid>
                                </Border>
                            </Grid>
                            <Grid x:Name="PART_gridRenderingRegion" FlowDirection="LeftToRight" Height="Auto" Margin="0,0,0,0" Grid.Row="3">
                                <Grid.Resources>
                                    <ResourceDictionary>
                                        <Style TargetType="TextBlock">
                                            <Setter Property="Foreground" Value="Black"></Setter>
                                        </Style>
                                    </ResourceDictionary>
                                </Grid.Resources>
                                <Grid>
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <Grid x:Name="PART_renderArea" Grid.Row="1">
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="Auto"/>
                                            <ColumnDefinition Width="*"/>
                                        </Grid.ColumnDefinitions>
                                        <Grid x:Name="PART_treeItemArea" Grid.Column="0" Height="Auto" Visibility="Collapsed" Width="Auto">
                                            <TreeView x:Name="PART_DocumentMap" BorderThickness="0" Height="Auto" Width="Auto"/>
                                        </Grid>
                                        <GridSplitter x:Name="PART_viewerSpliter" Background="{StaticResource DgAccent}" HorizontalAlignment="Stretch" ResizeBehavior="PreviousAndNext" ShowsPreview="True" VerticalAlignment="Stretch" Visibility="Collapsed" Width="5"/>

                                        <!--Viewer Page-->
                                        <ScrollViewer x:Name="PART_scrollViewer"  Grid.Column="2" Height="Auto" Width="Auto" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" HorizontalContentAlignment="Center" VerticalContentAlignment="Center">
                                            <ScrollViewer.Resources>
                                                <DataTemplate x:Key="ReportViewerCellTemplate">
                                                    <ContentControl Content="{Binding Path=CellBoundValue}"></ContentControl>
                                                </DataTemplate>
                                            </ScrollViewer.Resources>
                                            <StackPanel Orientation="Vertical" Name="PART_PageView" Height="Auto" Width="Auto" Background="White">

                                                <StackPanel.LayoutTransform>
                                                    <ScaleTransform x:Name="PART_Zoom"></ScaleTransform>
                                                </StackPanel.LayoutTransform>
                                                <Border x:Name="PART_PageViewBody" Background="Transparent" Height="Auto" Width="Auto">
                                                    <StackPanel x:Name="PART_PageViewContainer" Background="Transparent" Height="Auto" Width="Auto">

                                                        <Border Name="PART_PageHeaderBorder" Height="Auto" Width="Auto" Background="Transparent">
                                                            <Canvas x:Name="PART_CanvasHeader" Visibility="Collapsed" Background="Transparent"/>
                                                        </Border>
                                                        <Border Name="PART_PageBodyBorder" Height="Auto" Width="Auto" Background="Transparent">
                                                            <Canvas x:Name="PART_canvasContentPage" Grid.Row="2" Background="Transparent" Width="Auto" Height="Auto"/>
                                                        </Border>
                                                        <Border Name="PART_PageFooterBorder" Height="Auto" Width="Auto" Background="Transparent">
                                                            <Canvas x:Name="PART_CanvasFooter" Visibility="Collapsed" Background="Transparent"/>
                                                        </Border>
                                                    </StackPanel>
                                                </Border>
                                            </StackPanel>
                                        </ScrollViewer>
                                    </Grid>
                                </Grid>
                            </Grid>
                            <Grid x:Name="PART_gridException" Background="White" FlowDirection="LeftToRight" Grid.Row="4" Visibility="Collapsed">
                                <StackPanel HorizontalAlignment="Center" Orientation="Vertical">
                                    <StackPanel HorizontalAlignment="Center" Orientation="Horizontal">
                                        <Image x:Name="PART_imageException" HorizontalAlignment="Center" Source="{StaticResource Office2013Exception}" Width="48"/>
                                        <TextBlock x:Name="PART_textBlockException"/>
                                    </StackPanel>
                                    <ScrollViewer x:Name="PART_groupBoxExpandedExceptionScroll" HorizontalScrollBarVisibility="Auto" Height="400" VerticalScrollBarVisibility="Auto">
                                        <GroupBox x:Name="PART_groupBoxExpandedException" BorderThickness="0" FlowDirection="RightToLeft" Margin="5,0,5,0" Width="420">
                                            <GroupBox.Header>
                                                <ToggleButton x:Name="PART_toggleShowDetails" Content="Show Details" HorizontalAlignment="Right" Margin="0,0,0,0" VerticalAlignment="Bottom"/>
                                            </GroupBox.Header>
                                            <WrapPanel x:Name="PART_ShowError" Visibility="Collapsed" Width="auto">
                                                <TextBox x:Name="PART_textBlockStackTrace" FlowDirection="LeftToRight" IsReadOnly="True" TextWrapping="Wrap" Width="390"/>
                                                <TextBox x:Name="PART_textBlockStackTraceMessage" FlowDirection="LeftToRight" IsReadOnly="True" TextWrapping="Wrap" Width="390"/>
                                            </WrapPanel>
                                        </GroupBox>
                                    </ScrollViewer>
                                </StackPanel>
                            </Grid>
                            <Grid x:Name="PART_gridLoadingIndicator" Background="Black" FlowDirection="LeftToRight" Grid.Row="5" Visibility="Collapsed">
                                <!--<controls:LoadingIndicator Background="Black"/>-->
                                <ProgressBar Width="100" Height="100" IsIndeterminate="True" VerticalAlignment="Center" HorizontalAlignment="Center" Style="{StaticResource DgCircularProgressBar}"></ProgressBar>
                            </Grid>
                        </Grid>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="ViewMode" Value="Normal">
                            <Setter Property="Background" TargetName="PART_gridRenderingRegion" Value="{StaticResource DgBackground}"/>
                        </Trigger>
                        <Trigger Property="ViewMode" Value="Print">
                            <Setter Property="Background" TargetName="PART_gridRenderingRegion" Value="Black"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>

    </Style>
</ResourceDictionary>