  <DataSets>
    <DataSet Name="DataSetNameHere">
      <Query>
        <DataSourceName>LotteryRetail</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@StartDate">
            <Value>=Parameters!StartDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@EndDate">
            <Value>=Parameters!EndDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT        
PromoScheduleID, 
Comments, 
PromoStart, 
PromoEnd, 
PromoStarted, 
PromoEnded, 
CASE 
   WHEN PromoStarted = 0 THEN 'Not Started'
   WHEN PromoStarted = 1 AND PromoEnded = 0 Then 'Started'
   WHEN PromoStarted = 1 AND PromoEnded = 1 Then 'Ended'
   WHEN PromoStarted = 0 AND PromoEnded = 1 Then 'Promo Ended But Not Started.'   
End As PromoStatus,
TOTAL_PROMO_AMOUNT_TICKETS, 
TOTAL_PROMO_FACTOR_TICKETS, 
TOTAL_PROMO_AMOUNT_TICKETS + TOTAL_PROMO_FACTOR_TICKETS AS PromoScheduleTotal
FROM            PROMO_SCHEDULE
WHERE PromoStart Between @StartDate and @EndDate
     OR PromoEnd Between @StartDate and @EndDate</CommandText>
      </Query>
