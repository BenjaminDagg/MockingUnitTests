  <DataSets>
    <DataSet Name="ErrorTabsDetail">
      <Query>
        <DataSourceName>LotteryRetail</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@NumberOfReportingMonths">
            <Value>=Parameters!NumberOfReportingMonths.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@DateSelected">
            <Value>=Parameters!DateSelected.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>DECLARE @TabErrorTable TABLE
(LocationID INT, LocationName VARCHAR(48), ErrorCount INT, ErrorDescription VARCHAR(256),TotalPlayCount INT,
 DaysInPlay INT, AccountingMonth INT, AccountingYear INT)

 DECLARE @FirstMonth  DATETIME
 DECLARE @PeriodStart DATETIME
 DECLARE @PeriodEnd   DATETIME
 DECLARE @Month       INT
 DECLARE @Year        INT
 
 SET @FirstMonth = DATEADD(MONTH, -@NumberOfReportingMonths, @DateSelected)
 SET @PeriodStart = DATEADD(MONTH, DATEDIFF(MONTH, 0, @FirstMonth), 0)
 SET @PeriodEnd = DATEADD(MONTH, @NumberOfReportingMonths + 1, @PeriodStart)

 WHILE @PeriodStart &lt;&gt; @PeriodEnd
    BEGIN
       SET @Month = MONTH(@PeriodStart)
	    SET @Year = YEAR(@PeriodStart)

	    INSERT INTO @TabErrorTable
          (LocationID, LocationName, ErrorCount, ErrorDescription,
           TotalPlayCount, DaysInPlay, AccountingMonth, AccountingYear)
	   
	   SELECT
         (SELECT LOCATION_ID FROM dbo.CASINO WHERE SETASDEFAULT = 1) AS LocationID
         ,(SELECT CAS_NAME FROM dbo.CASINO WHERE SETASDEFAULT = 1) AS LocationName
         ,COUNT(te.TAB_ERROR_ID) AS ErrorCount
		   ,el.[DESCRIPTION] AS ErrorDescription

		   -- Get total number of plays...
         ,(SELECT SUM(PLAY_COUNT)
           FROM MACHINE_STATS
	        WHERE MONTH(ACCT_DATE) = @Month AND
	        YEAR(ACCT_DATE)  = @Year) AS TotalPlayCount
          
         -- Get total number of days where machines had plays...
         ,(SELECT COUNT(*) AS Plays FROM
		   (SELECT DISTINCT ACCT_DATE
		   FROM MACHINE_STATS
			WHERE PLAY_COUNT &gt; 0 AND
            MONTH(ACCT_DATE) = @Month AND
            YEAR(ACCT_DATE) = @Year) AS Dates) AS DaysInPlay
            ,@Month AS AccountingMonth
		      ,@Year AS AccountingYear
      
      FROM TAB_ERROR te
         JOIN ERROR_LOOKUP el ON te.ERROR_NO = el.ERROR_NO
      WHERE
         MONTH(dbo.ufnGetAcctDateFromDate(te.EVENT_TIME)) = @Month AND
         YEAR(dbo.ufnGetAcctDateFromDate(te.EVENT_TIME)) = @Year
      GROUP BY
         el.[DESCRIPTION]

      SET @PeriodStart = DATEADD(MONTH, 1, @PeriodStart)
   END

SELECT * FROM @TabErrorTable </CommandText>
      </Query>
