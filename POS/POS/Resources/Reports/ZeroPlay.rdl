  <DataSets>
    <DataSet Name="dsZeroPlay">
      <Query>
        <DataSourceName>LotteryRetail</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@AcctDate">
            <Value>=Parameters!AcctDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT
   ms.CASINO_MACH_NO,
   ms.MACH_NO,
   ISNULL(gs.GAME_DESC, 'Unknown') AS GAME_DESC,
   ISNULL(SUM(mstat.PLAY_COUNT), 0) AS PLAY_COUNT
FROM
   MACH_SETUP ms
   LEFT OUTER JOIN GAME_SETUP gs ON ms.GAME_CODE = gs.GAME_CODE
   LEFT OUTER JOIN dbo.MACHINE_STATS mstat ON ms.MACH_NO = mstat.MACH_NO AND
                                              mstat.ACCT_DATE = @AcctDate
WHERE
   (ms.MACH_NO &lt;&gt; '0') AND
   (ms.REMOVED_FLAG = 0)
GROUP BY
   ms.MACH_NO,
   ms.CASINO_MACH_NO,
   gs.GAME_DESC
HAVING
   ISNULL(SUM(mstat.PLAY_COUNT), 0) = 0
ORDER BY
   ms.CASINO_MACH_NO</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="CASINO_MACH_NO">
          <DataField>CASINO_MACH_NO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="MACH_NO">
          <DataField>MACH_NO</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="GAME_DESC">
          <DataField>GAME_DESC</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="PLAY_COUNT">
          <DataField>PLAY_COUNT</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
