<UserControl x:Class="POS.Modules.Settings.Views.SettingsView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:POS.Modules.Settings.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="1000">
    <ScrollViewer VerticalScrollBarVisibility="Auto">
        <StackPanel>
            <!--<local:CashDrawerView DataContext="{Binding}"  Visibility="{Binding HasCashDrawer, Converter={StaticResource BooleanToVisibility}}"/>-->
            <local:AdvancedSettingsView DataContext="{Binding}"/>
        </StackPanel>
    </ScrollViewer>
</UserControl>
